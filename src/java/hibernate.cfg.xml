<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/evoting_ms?zeroDateTimeBehavior=convertToNull</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password">12345</property>
    <mapping resource="entity/Mark.hbm.xml"/>
    <mapping resource="entity/AddCandidate1.hbm.xml"/>
    <mapping resource="entity/Admin.hbm.xml"/>
    <mapping resource="entity/AddCandidate.hbm.xml"/>
    <mapping resource="entity/Voting.hbm.xml"/>
    <mapping resource="entity/AddVoter.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
